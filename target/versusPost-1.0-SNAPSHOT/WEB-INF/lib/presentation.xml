<web-app>
	<display-name>
		My web app
	</display-name>
	<description>A web App</description>
	<session-config>
		<session-timeout>60</session-timeout>
	</session-config>
	<servlet>
		<servlet-name></servlet-name>
		<servlet-class>.class</servlet-class>
	<init-param>
		<param-name>foo</param-name>
		<param-value>bar</param-value>
	</init-param>
</servlet>
<servlet-mapping>
	
	<url-pattern>
	/Authentification
	</url-pattern>
</servlet-mapping>
<error-page>
	<exception-type>
		exception.DocNotFoundException
	</exception-type>
	<location>/error.html</location>
</error-page>
</web-app>


